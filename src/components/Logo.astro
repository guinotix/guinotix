---
import LogoExpress from './logos/LogoExpress.astro';
import LogoInertia from './logos/LogoInertia.astro';
import LogoLaravel from './logos/LogoLaravel.astro';
import LogoMySQL from './logos/LogoMySQL.astro';
import LogoReact from './logos/LogoReact.astro';
import LogoTailwind from './logos/LogoTailwind.astro';
import LogoStripe from './logos/LogoStripe.astro';
import LogoFilament from './logos/LogoFilament.astro';
import LogoPHP from './logos/LogoPHP.astro';
import LogoPython from './logos/LogoPython.astro';
import LogoJavascript from './logos/LogoJavascript.astro';
import LogoWordpress from './logos/LogoWordpress.astro';

const { name, ...props } = Astro.props;

const icons = {
    express: LogoExpress,
    inertia: LogoInertia,
    laravel: LogoLaravel,
    mysql: LogoMySQL,
    react: LogoReact,
    tailwind: LogoTailwind,
    stripe: LogoStripe,
    filament: LogoFilament,
    php: LogoPHP,
    python: LogoPython,
    javascript: LogoJavascript,
    wordpress: LogoWordpress,
};

const SelectedIcon = icons[name.toLowerCase()];
---

{SelectedIcon ? <SelectedIcon {...props} /> : null}
